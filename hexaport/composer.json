{
    "name": "hexaport/hexaport",
    "description": "Hexaport",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hexaport/hexaport/issues",
        "source": "https://github.com/hexaport/hexaport"
    },
    "authors": [
        {
            "name": "Necati Yared Ozal",
            "email": "hi@ozal.me",
            "homepage": "https://ozal.me"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "hexaport/support": "^1.0@dev",
        "laminas/laminas-json-server": "^3.2",
        "laminas/laminas-server": "^2.4",
        "laminas/laminas-soap": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Hexaport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hexaport\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "hexaport/metapackage": {
            "type": "path",
            "url": "../{,*/}"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
