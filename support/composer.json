{
    "name": "hexaport/support",
    "type": "library",
    "description": "Hexaport: Support Package",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hexaport/hexaport/issues",
        "source": "https://github.com/hexaport/hexaport"
    },
    "authors": [
        {
            "name": "Necati Yared Ozal",
            "email": "hi@ozal.me",
            "homepage": "https://ozal.me"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Hexaport\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hexaport\\Support\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "hexaport/metapackage": {
            "type": "path",
            "url": "../{,*/}"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
